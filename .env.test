# ============================================================
# AR FILTER SYSTEM - TEST CONFIGURATION
# ============================================================
# Acest fișier conține configurația pentru testare cu mock server
# 
# Pentru a activa: copiază acest fișier ca .env
# Windows: copy .env.test .env
# Linux/Mac: cp .env.test .env

# Environment Type
ENVIRONMENT=test

# Mock Server URLs (Local Testing)
CHATURBATE_URL=http://127.0.0.1:5000/events/chaturbate
STRIPCHAT_URL=http://127.0.0.1:5000/events/stripchat
CAMSODA_URL=http://127.0.0.1:5000/events/camsoda

# Platform Enable/Disable
CHATURBATE_ENABLED=true
STRIPCHAT_ENABLED=true
CAMSODA_ENABLED=true

# Key Automation Settings
KEYPRESS_HOLD_MS=50
KEYPRESS_DELAY_MS=80

# Debug Settings
DEBUG_MODE=true
VERBOSE_LOGGING=false

# Mock Server Settings
MOCK_SERVER_PORT=5000
MOCK_SERVER_HOST=127.0.0.1

# Queue UI Server Settings
QUEUE_UI_ENABLED=true
QUEUE_UI_HOST=127.0.0.1
QUEUE_UI_PORT=8080
QUEUE_UI_AUTO_START=false
